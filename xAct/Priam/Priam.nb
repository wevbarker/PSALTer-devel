 RowBox[{
  RowBox[{
   RowBox[{"SaturateMe", "[", "Expr_", "]"}], ":=", 
   RowBox[{"Catch", "@", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "SymbolicLagrangian", ",", "Symbols", ",", "MatrixLagrangian", ",", 
        "ImaginaryParts", ",", "RealParts", ",", "NullSpaces", ",", 
        "SourceConstraints", ",", "MatrixPropagator", ",", 
        "SaturatedPropagator", ",", "printer"}], "}"}], ",", "\n", 
      RowBox[{
       RowBox[{"printer", "=", 
        RowBox[{"{", "}"}]}], ";", "\n", 
       RowBox[{"printer", "=", 
        RowBox[{"printer", "~", "Append", "~", 
         RowBox[{"PrintTemporary", "@", "\"\< ** SaturateMe...\>\""}]}]}], 
       ";", "\n", "\n", 
       RowBox[{"(*", 
        RowBox[{"coefficient", " ", "matrices"}], "*)"}], "\n", 
       RowBox[{"SymbolicLagrangian", "=", 
        RowBox[{"Expr", "/.", "Global`cRules"}]}], ";", 
       RowBox[{"(*", 
        RowBox[{
        "remember", " ", "the", " ", "2", "m", " ", "mode", " ", "turns", " ",
          "out", " ", "to", " ", "vanish", " ", "once", " ", "you", " ", 
