Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"BuildPriam", "::", "usage"}], "=", 
   "\"\<BuildPriam[] introduces a few new definitions.\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"FourierLagrangian", "::", "usage"}], "=", 
   "\"\<FourierLagrangian[Expr,Tensor1,Tensor2,...] transfers a scalar \
expression Expr which is quadratic in the given perturbed fields whose \
xTensor heads are Tensor1 and Tensor2 into its Fourier form. \nBoth Expr and \
at least one field must be provided. Do not include indices in the fields, \
just list the xTensor heads (i.e. the tensor names).\>\""}], ";"}], "\n", 
 RowBox[{
  RowBox[{
   RowBox[{"SaturateMe", "::", "usage"}], "=", 
   "\"\<SaturateMe[Expr] produces the saturated propagator from the \
Lagrangian Expr, which must be a scalar of the form output by \
FourierLagrangian[].\>\""}], ";"}]}], "Code",
 InitializationCell->True,
