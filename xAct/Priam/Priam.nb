       RowBox[{"If", "[", 
        RowBox[{"xAct`HiGGS`Private`$HiGGSBuilt", ",", "Null", ",", 
         RowBox[{"Throw", "@", 
          RowBox[{"Message", "[", 
           RowBox[{"BuildPriam", "::", "unbuilt"}], "]"}]}], ",", 
