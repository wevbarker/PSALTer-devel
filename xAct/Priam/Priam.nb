Cell[BoxData[
 RowBox[{
  RowBox[{
   RowBox[{"FourierLagrangian", "[", 
    RowBox[{"Expr_", ",", "Tensors__"}], "]"}], ":=", 
   RowBox[{"Catch", "@", 
    RowBox[{"Module", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{
       "CrossingRules", ",", "ToMomentumExpr", ",", "Tensors1", ",", 
        "Tensors2", ",", "printer"}], "}"}], ",", "\n", 
      RowBox[{
       RowBox[{"printer", "=", 
        RowBox[{"{", "}"}]}], ";", "\n", 
       RowBox[{"printer", "=", 
        RowBox[{"printer", "~", "Append", "~", 
         RowBox[{
         "PrintTemporary", "@", "\"\< ** FourierLagrangian...\>\""}]}]}], ";",
        "\n", 
       RowBox[{"Tensors1", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"#", "@@", 
            RowBox[{"(", 
             RowBox[{"ToExpression", "/@", 
              RowBox[{
               RowBox[{"Alphabet", "[", "]"}], "[", 
               RowBox[{"[", 
                RowBox[{"1", ";;", 
                 RowBox[{"(", 
                  RowBox[{"Length", "@", 
                   RowBox[{"SlotsOfTensor", "@", "#"}]}], ")"}]}], "]"}], 
               "]"}]}], ")"}]}], ")"}], "&"}], "/@", 
         RowBox[{"(", 
          RowBox[{"List", "@", "Tensors"}], ")"}]}]}], ";", "\n", 
       RowBox[{"Tensors2", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"(", 
           RowBox[{"#", "@@", 
            RowBox[{"(", 
             RowBox[{"ToExpression", "/@", 
              RowBox[{
               RowBox[{"Alphabet", "[", "]"}], "[", 
               RowBox[{"[", 
                RowBox[{
                 RowBox[{"-", 
                  RowBox[{"(", 
                   RowBox[{"Length", "@", 
                    RowBox[{"SlotsOfTensor", "@", "#"}]}], ")"}]}], ";;", 
                 RowBox[{"-", "1"}]}], "]"}], "]"}]}], ")"}]}], ")"}], "&"}], 
         "/@", 
         RowBox[{"(", 
          RowBox[{"List", "@", "Tensors"}], ")"}]}]}], ";", "\n", 
       RowBox[{"CrossingRules", "=", 
        RowBox[{"{", "}"}]}], ";", 
       RowBox[{"(*", 
        RowBox[{
        "start", " ", "off", " ", "without", " ", "any", " ", "rules"}], 
        "*)"}], "\n", 
       RowBox[{"(*", 
        RowBox[{"first", " ", "the", " ", "double", " ", "derivatives"}], 
        "*)"}], "\n", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"CrossingRules", "=", 
           RowBox[{"CrossingRules", "~", "Join", "~", "\n", 
            RowBox[{"MakeRule", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"Evaluate", "[", 
                 RowBox[{
                  RowBox[{
                   RowBox[{"Global`CD", "[", 
                    RowBox[{"-", "Global`q"}], "]"}], "@", "Tensor1"}], " ", 
                  RowBox[{
                   RowBox[{"Global`CD", "[", 
                    RowBox[{"-", "Global`p"}], "]"}], "@", "Tensor2"}]}], 
                 "]"}], ",", 
                RowBox[{"Evaluate", "[", 
                 RowBox[{
                  RowBox[{"Dagger", "@", "Tensor1"}], " ", 
                  RowBox[{"Global`P", "[", 
                   RowBox[{"-", "Global`p"}], "]"}], 
                  RowBox[{"Global`P", "[", 
                   RowBox[{"-", "Global`q"}], "]"}], "Tensor2"}], "]"}]}], 
               "}"}], ",", 
              RowBox[{"MetricOn", "\[Rule]", "All"}], ",", 
              RowBox[{"ContractMetrics", "\[Rule]", "True"}]}], "]"}]}]}], 
          ")"}], ",", "\n", 
         RowBox[{"{", 
          RowBox[{"Tensor1", ",", "Tensors1"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"Tensor2", ",", "Tensors2"}], "}"}]}], "]"}], ";", "\n", 
       RowBox[{"(*", 
        RowBox[{"then", " ", "single", " ", "derivatives"}], "*)"}], "\n", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"CrossingRules", "=", 
           RowBox[{"CrossingRules", "~", "Join", "~", "\n", 
            RowBox[{"MakeRule", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"Evaluate", "[", 
                 RowBox[{"Tensor1", " ", 
                  RowBox[{
                   RowBox[{"Global`CD", "[", 
                    RowBox[{"-", "Global`p"}], "]"}], "@", "Tensor2"}]}], 
                 "]"}], ",", 
                RowBox[{"Evaluate", "[", 
                 RowBox[{
                  RowBox[{"-", "I"}], " ", 
                  RowBox[{"Dagger", "@", "Tensor1"}], " ", 
                  RowBox[{"Global`P", "[", 
                   RowBox[{"-", "Global`p"}], "]"}], "Tensor2"}], "]"}]}], 
               "}"}], ",", 
              RowBox[{"MetricOn", "\[Rule]", "All"}], ",", 
              RowBox[{"ContractMetrics", "\[Rule]", "True"}]}], "]"}]}]}], 
          ")"}], ",", "\n", 
         RowBox[{"{", 
          RowBox[{"Tensor1", ",", "Tensors1"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"Tensor2", ",", "Tensors2"}], "}"}]}], "]"}], ";", "\n", 
       RowBox[{"(*", 
        RowBox[{"and", " ", "finally", " ", "algebraic", " ", "products"}], 
        "*)"}], "\n", 
       RowBox[{"Table", "[", 
        RowBox[{
         RowBox[{"(", 
          RowBox[{"CrossingRules", "=", 
           RowBox[{"CrossingRules", "~", "Join", "~", "\n", 
            RowBox[{"MakeRule", "[", 
             RowBox[{
              RowBox[{"{", 
               RowBox[{
                RowBox[{"Evaluate", "[", 
                 RowBox[{"Tensor1", " ", "Tensor2"}], "]"}], ",", 
                RowBox[{"Evaluate", "[", 
                 RowBox[{
                  RowBox[{"Dagger", "@", "Tensor1"}], " ", "Tensor2"}], 
                 "]"}]}], "}"}], ",", 
              RowBox[{"MetricOn", "\[Rule]", "All"}], ",", 
              RowBox[{"ContractMetrics", "\[Rule]", "True"}]}], "]"}]}]}], 
          ")"}], ",", "\n", 
         RowBox[{"{", 
          RowBox[{"Tensor1", ",", "Tensors1"}], "}"}], ",", 
         RowBox[{"{", 
          RowBox[{"Tensor2", ",", "Tensors2"}], "}"}]}], "]"}], ";", "\n", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"Expr", "/.", "CrossingRules"}]}], ";", 
       RowBox[{"(*", 
        RowBox[{
        "now", " ", "impose", " ", "these", " ", "rules", " ", "to", " ", 
         "obtain", " ", "Fourier", " ", "space", " ", "version"}], "*)"}], 
       "\n", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "//", "ToNewCanonical"}]}], ";", "\n", 
       RowBox[{"printer", "=", 
        RowBox[{"printer", "~", "Append", "~", 
         RowBox[{"PrintTemporary", "@", "ToMomentumExpr"}]}]}], ";", "\n", 
       RowBox[{"(*", 
        RowBox[{"now", " ", "move", " ", "over", " ", "to", " ", "SO", 
         RowBox[{"(", "3", ")"}], " ", "decomposition"}], "*)"}], "\n", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "/.", "Global`ToV"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "//", "ToNewCanonical"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{
         RowBox[{"ToMomentumExpr", "/.", "Global`GaugeDecompose"}], "//", 
         "ToNewCanonical"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{
         RowBox[{
          RowBox[{"ToMomentumExpr", "/.", "Global`GaugePToGaugePO3"}], "/.", 
          "Global`GaugePerpToGaugePO3"}], "//", "ToNewCanonical"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "//", "CollectTensors"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "/.", "Global`Patch2m"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "/.", "Global`ManualAll"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "/.", "Global`ManualAll"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "//", "ToNewCanonical"}]}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"ToMomentumExpr", "=", 
        RowBox[{"ToMomentumExpr", "//", "CollectTensors"}]}], ";", "\n", 
       RowBox[{"NotebookDelete", "@", "printer"}], ";", "\n", 
       "ToMomentumExpr"}]}], "]"}]}]}], ";"}]], "Code",
